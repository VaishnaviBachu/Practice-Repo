<aura:component controller="RestApiShiva" implements="flexipage:availableForAllPageTypes" access="global">
    <aura:attribute name="Names" type="RestApiShiva.accountRecord[]" default=""/>
	<aura:handler name="init" value="{!this}" action="{!c.doInit}"/> 
    
    <div class="slds"> 

    <table class="slds-table slds-table--bordered slds-table--striped">
        <thead>
            <tr>
                
                <th scope="col"><span class="slds-truncate">Account Names</span></th>
            </tr>
        </thead>
        <tbody>
            <aura:iteration items="{!v.Names}" var="accname">
                <tr>
                    
                    <td>{!accname.Name}</td>
                </tr>
            </aura:iteration>
        </tbody>
    </table>
        </div>
</aura:component>